<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class='col-md-12'>
  <%= will_paginate %>
</div>

<div class='row'>
  <ul class='users col-md-12'>
    <% @users.each do |user| %>
      <div class='col-md-4'>
        <%= render user %>
      </div>
    <% end %>
  </ul>
</div>

<div class='col-md-12'>
  <%= will_paginate %>
</div>
